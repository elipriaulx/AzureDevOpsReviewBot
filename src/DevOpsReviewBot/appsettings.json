{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AzureDevOps": {
    "OrganizationUrl": "https://dev.azure.com/your-organization",
    "PersonalAccessToken": "",
    "Repositories": [
      {
        "Project": "YourProject",
        "Repository": "YourRepository"
      }
    ]
  },
  "Review": {
    "PollingIntervalMinutes": 5,
    "CommentPrefix": "[DevOpsReviewBot]",
    "MaxCommentsPerFile": 5,
    "MaxFileSizeKb": 500,
    "ExcludePatterns": [
      "*.generated.cs",
      "*.Designer.cs",
      "*.g.cs",
      "package-lock.json",
      "yarn.lock",
      "pnpm-lock.yaml",
      "*.min.js",
      "*.min.css"
    ]
  },
  "CursorCli": {
    "TimeoutSeconds": 300,
    "MaxRetries": 3,
    "ModelName": null,
    "AgentCommand": "agent"
  }
}
